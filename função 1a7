questão 3 -

programa
{
    inclua biblioteca Util    
    
    funcao inicio()
    {
      
        km_por_litro(636)
        km_por_litro(940)
    
    }

    funcao km_por_litro (inteiro kilometros){
        inteiro gasolina = 50
          escreva("você usára " , kilometros / gasolina, " de gasolina para percorrer ",kilometros," km\n")
          
    }
}

questão 4 -

programa
{
    
    funcao inicio()
    {
      
     verficaLatido("Bob", 20)
    verficaLatido("Laika", 17.9)
    verficaLatido("Chica", 1.1)
    verficaLatido("Walter Geoffrey", 13)
    
    }

    funcao verficaLatido (cadeia nome, real peso){
        escreva("o nome do cachorro é ", nome, "e seu peso é ", peso, "\n")
       
          
    }
}

questão 5 - 
programa
{
	
	funcao inicio()
	{
		calculadora()
	
	}
    funcao calculadora(){
	    	inteiro x , y
	    	inteiro opcao
	    	leia(x)
	    	leia(y)

	    	leia(opcao)

	    	escolha(opcao){
	    		caso 1 : soma (x , y)
	    		pare

	    		caso 2 : subtracao (x , y)
	    		pare

	    		caso 3 : divisao (x , y)
	    		pare
	    		
	    		caso 4 : mutiplicacao (x , y)
	    		pare

	    		caso contrario: escreva("contrario")
	    		pare
	    	}
	    }

      	funcao soma (inteiro x, inteiro y){
		 escreva (" o resultado é: ", x + y)
			
		}

		funcao subtracao (inteiro x, inteiro y){
		  escreva (" o resultado é: ", x - y)
		
	}
      	funcao divisao (inteiro x, inteiro y){
		  escreva (" o resultado é: ", x / y)

      	}
      	funcao mutiplicacao (inteiro x, inteiro y){
		  escreva (" o resultado é: ", x * y)
      	}
}



